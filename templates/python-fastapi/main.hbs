{{!-- main.hbs --}}
from fastapi import FastAPI
from apis import router

app = FastAPI(
    title="{{info.title}}",
    description="{{info.description}}",
    version="{{info.version}}"
)

app.include_router(router)

if __name__ == "__main__":
    import uvicorn
    uvicorn.run(app, host="0.0.0.0", port=8000)
